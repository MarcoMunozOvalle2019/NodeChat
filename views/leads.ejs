<!DOCTYPE html>
<html>
  <head>
    <title>Socket.IO chat</title>
    <style>
      body { margin: 0; padding-bottom: 3rem; font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif; }

      #form { background: rgba(0, 0, 0, 0.15); padding: 0.25rem; position: fixed; bottom: 0; left: 0; right: 0; display: flex; height: 3rem; box-sizing: border-box; backdrop-filter: blur(10px); }
      #input { border: none; padding: 0 1rem; flex-grow: 1; border-radius: 2rem; margin: 0.25rem; }
      #input:focus { outline: none; }
      #form > button { background: #333; border: none; padding: 0 1rem; margin: 0.25rem; border-radius: 3px; outline: none; color: #fff; }

      #messages { list-style-type: none; margin: 0; padding: 0; }
      #messages > li { padding: 0.5rem 1rem; }
      #messages > li:nth-child(odd) { background: #efefef; }
    </style>
  </head>
  
  <body>
    <ul id="messages"></ul>
    <script src="/socket.io/socket.io.js"></script>

    <!-- <% console.log('aca',leads) %> -->
    
    <script>      
      var socket = io();
    </script>
    <script>      
      ///socket.emit('chat message', 'al inicio1');  
      let r=0
      socket.on('chat message', (msg)=> {
        {
          const ff = 'TT'+JSON.stringify(msg)+'GG'
   console.log('delphi',ff)
          var V1 = msg.split(':')[1].split(';')[0];
          var V2 = msg.split(':')[2].split(';')[0]; 
          var V3 = msg.split(':')[3].split(';')[0]; 
          var V4 = msg.split(':')[4].split(';')[0]; 
          var V5 = msg.split(':')[5].split(';')[0]; 
          var V6 = msg.split(':')[6].split(';')[0]; 
          var V7 = msg.split(':')[7].split(';')[0]; 
          var V8 = msg.split(':')[8].split(';')[0]; 
          var V9 = msg.split(':')[9].split(';')[0]; 
          var V10 = msg.split(':')[10].split(';')[0]; 
          var N1 = msg.split(':')[11].split(';')[0];
          var N2 = msg.split(':')[12].split(';')[0]; 
          var N3 = msg.split(':')[13].split(';')[0]; 
          var N4 = msg.split(':')[14].split(';')[0]; 
          var N5 = msg.split(':')[15].split(';')[0]; 
          var N6 = msg.split(':')[16].split(';')[0]; 
          var N7 = msg.split(':')[17].split(';')[0]; 
          var N8 = msg.split(':')[18].split(';')[0]; 
          var N9 = msg.split(':')[19].split(';')[0]; 
          var N10 = msg.split(':')[20].split(';')[0]; 
          // console.log('J1',N1)
          // console.log('J2',N2)
          // console.log('J3',N3)
          
          document.getElementById("demo1").innerHTML = N1+'= '+V1;
          document.getElementById("demo2").innerHTML = N2+'= '+V2;
          document.getElementById("demo3").innerHTML = N3+'= '+V3;
          document.getElementById("demo4").innerHTML = N4+'= '+V4;
          document.getElementById("demo5").innerHTML = N5+'= '+V5;
          document.getElementById("demo6").innerHTML = N6+'= '+V6;
          document.getElementById("demo7").innerHTML = N7+'= '+V7;
          document.getElementById("demo8").innerHTML = N8+'= '+V8;
          document.getElementById("demo9").innerHTML = N9+'= '+V9;
          document.getElementById("demo10").innerHTML = N10+'= '+V10;
        }
          r=r+1
      });
      
    </script>

